{
  "multipart": [
    {
      "when": {
        "facing": "north", "open": "true"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_open",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "north", "open": "false"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_closed",
        "y": 180
      }
    },

    {
      "when": {
        "facing": "east", "open": "true"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_open",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "east", "open": "false"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_closed",
        "y": 270
      }
    },

    {
      "when": {
        "facing": "south", "open": "true"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_open"
      }
    },
    {
      "when": {
        "facing": "south", "open": "false"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_closed"
      }
    },

    {
      "when": {
        "facing": "west", "open": "true"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_open",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west", "open": "false"
      },
      "apply": {
        "model": "hotchicks:block/hutch_gate_closed",
        "y": 90
      }
    }
  ]
}