{
  "multipart": [
    { "when": { "facing": "north" },
      "apply": { "model": "hotchicks:block/trellis_empty" }
    },

    { "when": { "facing": "south" },
      "apply": { "model": "hotchicks:block/trellis_empty", "y": 180 }
    },

    { "when": { "facing": "east" },
      "apply": { "model": "hotchicks:block/trellis_empty", "y": 90 }
    },

    { "when": { "facing": "west" },
      "apply": { "model": "hotchicks:block/trellis_empty", "y": 270 }
    }
  ]
}